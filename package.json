  "dependencies": {
    "@rollup/pluginutils": "^5.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.11.18",
    "rollup": "^3.10.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "vite": ">=2.0.0"
